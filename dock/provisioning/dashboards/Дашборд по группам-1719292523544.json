          "rawSql": "SELECT\r\n\tmain.student_id as \"ID студента\",\r\n\tstudent_name as \"ФИО студента\",\r\n\tgroup_names as \"Группа\",\r\n\tstudent_presence.student_presence_percent as \"Процент посещаемости\",\r\n\ttotal_score as \"Итоговый балл\"\r\nFROM (\r\n\tSELECT s.student_id, s.student_name, STRING_AGG(study_group_name, ', ') as group_names, total_score FROM uploaded_db_result dbr\r\n\tJOIN student s ON dbr.student_id = s.student_id\r\n\tJOIN study_group sg ON dbr.study_group_id = sg.study_group_id\r\n\tWHERE uploaded_db_id = $uploaded_db_id AND study_group_name = ANY(ARRAY[${group_name:sqlstring}])\r\n\tGROUP BY s.student_id, total_score\r\n\t ) main\r\nJOIN (\r\n\tSELECT\r\n\t\tstudent_id,\r\n\t\tCASE WHEN SUM(CASE WHEN presence = true OR presence = false THEN 1 ELSE 0 END) = 0 THEN NULL\r\n\t\tELSE ROUND(SUM(CASE WHEN presence = true THEN 1 ELSE 0 END) * 100.0 / SUM(CASE WHEN presence = true OR presence = false THEN 1 ELSE 0 END), 2) END AS student_presence_percent,\r\n\t\tuploaded_db_id\r\n\tFROM student_db_presence\r\n\tWHERE uploaded_db_id = $uploaded_db_id AND study_group_name = ANY(ARRAY[${group_name:sqlstring}])\r\n\tGROUP BY student_id, uploaded_db_id\r\n) student_presence ON main.student_id = student_presence.student_id",
