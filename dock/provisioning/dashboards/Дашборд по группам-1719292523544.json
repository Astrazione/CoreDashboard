          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n\tmain.study_group_name as \"Название группы\",\r\n\tteacher_name as \"Преподаватели\",\r\n\tstudents_count as \"Количество студентов\",\r\n\tgroup_presence.group_presence_percent as \"Процент посещаемости\",\r\n\tavg_total_score as \"Средний итоговый балл\"\r\nFROM (\r\n\tSELECT study_group_id, study_group_name, AVG(total_score) as avg_total_score, COUNT(*) as students_count \r\n\tFROM (\r\n\t\tSELECT dbr.study_group_id, total_score, study_group_name, student_id FROM uploaded_db_result dbr\r\n\t\tJOIN study_group sg ON dbr.study_group_id = sg.study_group_id\r\n\t\tWHERE uploaded_db_id = $uploaded_db_id AND study_group_name = ANY(ARRAY[${group_name:sqlstring}])\r\n\t\tGROUP BY student_id, dbr.study_group_id, total_score, study_group_name, student_id\r\n\t)\r\n\tGROUP BY study_group_id, study_group_name\r\n) main\r\nJOIN (\r\n\tSELECT\r\n\t\tstudy_group_id,\r\n\t\tCASE WHEN SUM(CASE WHEN presence = true OR presence = false THEN 1 ELSE 0 END) = 0 THEN NULL\r\n\t\tELSE ROUND(SUM(CASE WHEN presence = true THEN 1 ELSE 0 END) * 100.0 / \r\n    SUM(CASE WHEN presence = true OR presence = false THEN 1 ELSE 0 END), 2) END AS group_presence_percent\r\n\tFROM group_db_presence\r\n\tWHERE uploaded_db_id = $uploaded_db_id AND study_group_name = ANY(ARRAY[${group_name:sqlstring}])\r\n\tGROUP BY study_group_id\r\n) group_presence ON main.study_group_id = group_presence.study_group_id\r\nJOIN study_group sd ON main.study_group_id = sd.study_group_id\r\nJOIN teacher t ON sd.teacher_id = t.teacher_id",
          "refId": "A",
          "sql": {
            "columns": [
